{
  "name": "earthfrontend",
  "version": "0.0.0",
  "private": true,
  "main": "src/server/server.js",
  "author": "muwenzi",
  "description": "muwenzi.com version3",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "webpack": "better-npm-run webpack",
    "webpack:dashboard": "better-npm-run webpack:dashboard",
    "dev": "better-npm-run dev",
    "dev:dashboard": "better-npm-run dev:dashboard",
    "presandbox": "better-npm-run presandbox",
    "sandbox": "better-npm-run sandbox",
    "sandbox:debug": "better-npm-run sandbox:debug",
    "preprod": "better-npm-run preprod",
    "prod": "better-npm-run prod",
    "check:conflict": "scripty",
    "check:eslint": "scripty"
  },
  "betterScripts": {
    "webpack": {
      "command": "node-dev webpack/webpack.server.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 3000,
        "TIMEOUT": 2000
      }
    },
    "webpack:dashboard": {
      "command": "webpack-dashboard -- node-dev webpack/webpack.server.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 3000,
        "TIMEOUT": 2000
      }
    },
    "dev": {
      "command": "node $NODE_DEBUG_OPTION src/server/babel.register.js",
      "env": {
        "NODE_ENV": "development",
        "FRONTEND_PORT": 8890,
        "WEBPACK_PORT": 3000,
        "BACKEND_PORT": 8888
      }
    },
    "dev:dashboard": {
      "command": "nodejs-dashboard --port=8002 -- node -r nodejs-dashboard $NODE_DEBUG_OPTION src/server/babel.register.js",
      "env": {
        "NODE_ENV": "development",
        "FRONTEND_PORT": 8890,
        "WEBPACK_PORT": 3000,
        "BACKEND_PORT": 8888
      }
    },
    "presandbox": {
      "command": "webpack --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "sandbox"
      }
    },
    "sandbox": {
      "command": "pm2 start src/server/server.js --name 'earth-sanbox'",
      "env": {
        "NODE_ENV": "sandbox",
        "PORT": 8890
      }
    },
    "sandbox:debug": {
      "command": "npm run presandbox && node $NODE_DEBUG_OPTION src/server/server.js",
      "env": {
        "NODE_ENV": "sandbox",
        "PORT": 8890
      }
    },
    "preprod": {
      "command": "webpack --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod": {
      "command": "pm2 start src/server/server.js --name 'earth-production'",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8890
      }
    }
  },
  "scripty": {
    "silent": true
  },
  "pre-commit": [
    "check:conflict",
    "check:eslint"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "^0.1.0",
    "classlist": "^1.0.1",
    "classnames": "^2.2.5",
    "config-lite": "^1.6.0",
    "css-loader": "^0.28.7",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-winston": "^2.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-proxy-middleware": "^0.17.4",
    "invariant": "^2.2.2",
    "isomorphic-style-loader": "^4.0.0",
    "jsx-loader": "^0.13.2",
    "justified-layout": "^2.1.1",
    "less": "^2.7.3",
    "less-loader": "^3.0.0",
    "lodash": "^4.17.4",
    "nodejs-dashboard": "^0.4.3",
    "pm2": "^2.4.6",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-avatar-editor": "^10.3.0",
    "react-css-modules": "^4.2.0",
    "react-dom": "^16.1.1",
    "react-flexbox-grid": "^2.0.0",
    "react-grid-gallery": "^0.4.8",
    "react-masonry-component": "^6.0.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "react-sticky": "^6.0.1",
    "react-update": "^0.4.4",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "res.api": "^1.0.11",
    "scripty": "^1.7.1",
    "serve-favicon": "^2.4.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "istanbul": "0.4.5",
    "node-dev": "^3.1.3",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "supertest": "^3.0.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
